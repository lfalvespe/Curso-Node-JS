<div class="alunos">
    <h2>ALUNOS</h2>

    <div class="alunos_container">
        <h4>Lista de Alunos</h4>

        <table>
            <thead>
                <tr>
                    <th>id</th>
                    <th>aluno</th>
                    <th>idade</th>
                    <th>série</th>
                    <th>nível</th>
                    <th>email</th>
                </tr>
            </thead>
            <tbody>
                {{#each alunos}}
                <tr>
                    <td>{{this.id}}</td>
                    <td class="name">{{this.name}}</td>
                    <td>{{this.age}}</td>
                    <td>{{this.grade}}</td>
                    <td class="level">{{this.level}}</td>
                    <td>{{this.email}}</td>
                </tr>
                {{/each}}
            </tbody>
        </table>
        
        <br>

        <div class="btn_cadastrar"><a href="/" >Cadastrar novo aluno</a></div>

    </div>

</div>